<title>HISPIRE | {{{__ 'apicatando title'}}}</title>
<div id="bg__services" class="bg--full-fixed"></div>
{{> menu}}
<main id="porfolio" class="animated fadeIn">
  <div class="section__header">
    <h1 class="small-11 columns small-centered section__title animated fadeInDown text-center">
      {{{__ 'apicatando project title'}}}
    </h1>
  </div>
  <section id="project" class="no-bullet project">
    <div class="row project__intro">
      <div class="small-12 medium-10 large-8- xlarge-6 small-centered columns">
        <h2 class="">{{{__ 'the project'}}}</h2>
        <p>
          {{{__ 'apicatando project intro'}}}
        </p>
        <p>
          {{{__ 'apicatando project intro2'}}}
        </p>
        <h4 class="text-center">{{{__ 'explore project'}}}</h4>
        <div class="small-4 columns" data-tab="#website">
          <button type="button" class="button button--border small-12 active" name="button">
            {{{__ 'project webs'}}}
          </button>
        </div>
        <div class="small-4 columns" data-tab="#branding">
          <button type="button" class="button button--border small-12" name="button">
            {{{__ 'project branding'}}}
          </button>
        </div>
        <div class="small-4 columns" data-tab="#content">
          <button type="button" class="button button--border small-12" name="button">
            {{{__ 'project content'}}}
          </button>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="small-12 xlarge-10 small-centered columns">
        <div id="website" class="project__section">
          <div class="small-12 large-6 columns">
            <h3 id="websites" class="subheader">{{{__ 'project web title'}}}</h3>
            <p>
              {{{__ 'project web apicatando text'}}}
            </p>
          </div>
          <div class="small-12 large-6 columns">
            <img src="/img/porfolio/apicatando-resposive2.jpg" alt="" />
          </div>
        </div>
        <div id="branding" class="project__section">
          <div class="small-12 large-6 columns">
            <h3 class="subheader">{{{__ 'project branding title'}}}</h3>
            <h4><strong>{{{__ 'project branding apicatando tagline'}}} </strong></h4>
            <p id="logo-design">
              {{{__ 'project branding apicatando text'}}}
            </p>
          </div>
          <div class="small-12 large-6 columns">
            <img src="/img/porfolio/apicatando-branding.jpg" alt="" />
          </div>
          <div class="small-12 large-6 large-push-6 columns">
            <h3 class="subheader">{{{__ 'project logo title'}}}</h3>
            <p>
              {{{__ 'project logo apicatando text'}}}
            </p>
            <p>
              <i>{{{__ 'project logo apicatando text2'}}}</i>
            </p>
          </div>
          <div class="small-12 large-6 large-pull-6 columns">
            <img src="/img/porfolio/apicatando-logo.jpg" alt="" />
          </div>
          <div class="small-12 large-6 columns">
            <h3 class="subheader">{{{__ 'project design title'}}}</h3>
            <p id="graphic-design">
              {{{__ 'project design apicatando text'}}}
            </p>
          </div>
          <div class="small-12 large-6 columns">
            <img src="/img/porfolio/apicatando-icons.jpg" alt="" />
          </div>
        </div>
        <div id="content" class="project__section">
          <div class="small-12 large-6 columns">
            <h3 class="subheader">{{{__ 'project copywriting title'}}}</h3>
            <p>
              {{{__ 'project copywriting apicatando text'}}}
            </p>
          </div>
          <div class="small-12 large-6 columns">
            <img src="/img/porfolio/apicatando-layers.jpg" alt="" />
          </div>
          <div class="small-12 large-6 large-push-6 columns">
            <h3 class="subheader">{{{__ 'project photo title'}}}</h3>
            <p>
              {{{__ 'project photo apicatando text'}}}
            </p>
          </div>
          <div class="small-12 large-6 large-pull-6 columns">
            <img src="/img/porfolio/apicatando-photography.jpg" alt="" />
          </div>
        </div>
      </div>
    </div>
  </section>
  <div class="row">
    <div class="small-11 small-centered columns link__section text-center">
      <h3 class="wow fadeIn" data-wow-duration="1s" data-wow-delay=".1s">
        {{{__ 'project footer text'}}}
      </h3>
      <div class="wow fadeInLeft" data-wow-duration="1s" data-wow-delay=".1s">
        <a data-pjax href="{{lg}}start-project">
            {{{__ 'project footer action'}}}
        </a>
      </div>
    </div>
  </div>
</main>
<script type="text/javascript">
  $(function() {
    $('[data-hover="portfolio"]').addClass('active');
    $('[data-tab]').click(function(e) {
      $('[data-tab] button').removeClass('active');
      $(this).children().addClass('active');
      var id = $(this).attr('data-tab');
      $(id).siblings().hide();
      $(id).show().addClass("animated fadeIn");
    })
  })
</script>
{{> ui}}
